<xs:element name="RetrieveCasesStatus_Response" type="RetrieveCasesStatusResponse" />
    <xs:complexType name="RetrieveCasesStatusResponse">
        <xs:sequence>
            <xs:element minOccurs="1" name="Timestamp" type="xs:dateTime"/>
            <xs:element minOccurs="1" maxOccurs="1" name="ImageReviewers">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element minOccurs="1" maxOccurs="unbounded" name="ImageReviewer" >
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element minOccurs="1" name="ImageReviewerID" type="xs:string"/>
                                <xs:element minOccurs="1" name="ImageReviewerName" type="xs:string"/>
                                <xs:element minOccurs="1" name="StartTime" type="xs:dateTime"/>
                                <xs:element minOccurs="0" name="EndTime" type="xs:date"/>
                                <xs:element minOccurs="0" name="Cases">
                                    <xs:complexType>
                                        <xs:sequence>
                                            <xs:element minOccurs="0" maxOccurs="unbounded" name="Case">
                                                <xs:complexType>
                                                    <xs:sequence>
                                                        <xs:element name="CaseID" type="xs:integer" minOccurs="1" maxOccurs="1"/>
                                                        <xs:element name="CaseStartTime" type="xs:dateTime" minOccurs="0"/>
                                                        <xs:element name="CaseEndTime" type="xs:dateTime" minOccurs="0"/>
                                                        <xs:element name="VerdictType" type="xs:string" minOccurs="0"/>
                                                    </xs:sequence>
                                                </xs:complexType>
                                            </xs:element>
                                        </xs:sequence>
                                    </xs:complexType>
                                </xs:element>
                            </xs:sequence>
                        </xs:complexType>
                        </xs:element>

                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>


    <xs:element name="RetrieveCasesStatus_FailureResponse" type="RetrieveCasesStatusFailureResponse" />
    <xs:complexType name="RetrieveCasesStatusFailureResponse">
        <xs:sequence>
            <xs:element name="Message_Header">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element minOccurs="1" name="RequestID" type="xs:string" />
                        <xs:element minOccurs="1" name="Timestamp" type="xs:dateTime"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="Message_Body">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element minOccurs="1" name="Failure_Code" type="xs:string"/>
                        <xs:element minOccurs="1" name="Failure_Reason" type="xs:string"/>
                        <xs:element minOccurs="1" name="Severity" type="xs:string"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

</xs:schema>